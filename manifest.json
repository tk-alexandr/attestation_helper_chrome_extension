{
    "manifest_version": 2,
    "name": "Att helper",
    "version": "1.0",
    
    "icons": {
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": [ "https://att.elma-bpm.ru/*", "http://localhost:1111/test_pages/**" ],
        "js": ["jquery-1.12.4.min.js", "content.js" ]
      }
    ],
    "background": {
      "scripts": ["jquery-1.12.4.min.js", "background.js"]
    },
    "permissions": [
      "https://att.elma-bpm.ru/*",
      "http://localhost:1111/test_pages/**",
      "tabs",
      "activeTab"
    ],
  
        "browser_action": {
          "default_title": "Att helper",
          "default_icon": "icon.png",
          "default_popup": "popup.html"
      }
      
  }